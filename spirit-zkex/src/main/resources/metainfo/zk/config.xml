<?xml version="1.0" encoding="UTF-8"?>
<config>
    <config-name>spirit-zkex</config-name><!-- used to resolve dependency -->
    <depends>zkbind</depends>

    <version>
        <version-class>com.googlecode.easyec.zkex.Version</version-class>
        <version-uid>0.8.2</version-uid>
    </version>

    <listener>
        <listener-class>com.googlecode.easyec.zkex.bind.ZkexPhaseListener</listener-class>
    </listener>

    <library-property>
        <name>org.zkoss.bind.ReferenceBindingHandler.class</name>
        <value>com.googlecode.easyec.zkex.bind.impl.ReferenceBindingHandlerImpl</value>
    </library-property>
</config>
